    if (workcell == "workcell1") {
        $("#jb1").attr("href", "/Login/UserManage");
        $("#jb2").attr("href", "/Login/GroupManage");
    }
    if (workcell == "workcell2") {
        $("#jb1").attr("href", "https://baidu.com");
        $("#jb2").attr("href", "https://www.csgo.com.cn/index.html");
    }
    if (workcell == "workcell3") {
        $("#jb1").attr("href", "http://jx3.xoyo.com/");
        $("#jb2").attr("href", "https://baike.baidu.com/item/%E5%8F%8D%E6%81%90%E7%B2%BE%E8%8B%B1/69155?fr=aladdin");
    }
</script>